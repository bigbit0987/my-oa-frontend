<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" 
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
             xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" 
             xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" 
             xmlns:flowable="http://flowable.org/bpmn" 
             targetNamespace="http://www.company.com/workflow" 
             exporter="Camunda Modeler" 
             exporterVersion="5.41.0">

  <process id="archiving_transfer_checklist_process" name="项目归档移交记录清单流程" isExecutable="true">
    <documentation>
      归档评分阶段 - 项目归档移交记录清单（自动触发）
      流程说明：项目负责人填写移交清单 -> 信息中心确认
    </documentation>

    <startEvent id="startEvent" name="开始" flowable:initiator="initiator" />
    
    <sequenceFlow id="flow_start_to_pm" sourceRef="startEvent" targetRef="task_pm_fill" />

    <userTask id="task_pm_fill" name="项目负责人填写移交清单" flowable:assignee="${initiator}" flowable:formKey="archiving_transfer_checklist">
      <documentation>项目负责人填写文件移交清单</documentation>
    </userTask>

    <sequenceFlow id="flow_pm_to_info" sourceRef="task_pm_fill" targetRef="task_info_confirm" />

    <userTask id="task_info_confirm" name="信息中心确认" flowable:candidateGroups="INFO_CENTER" flowable:formKey="archiving_transfer_checklist">
      <documentation>信息中心确认归档移交</documentation>
    </userTask>

    <sequenceFlow id="flow_info_to_end" sourceRef="task_info_confirm" targetRef="endEvent" />

    <endEvent id="endEvent" name="结束" />

  </process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_arch_trans">
    <bpmndi:BPMNPlane id="BPMNPlane_arch_trans" bpmnElement="archiving_transfer_checklist_process">
      <bpmndi:BPMNShape id="shape_startEvent" bpmnElement="startEvent">
        <omgdc:Bounds x="150" y="200" width="30" height="30" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_task_pm_fill" bpmnElement="task_pm_fill">
        <omgdc:Bounds x="230" y="185" width="100" height="60" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_task_info_confirm" bpmnElement="task_info_confirm">
        <omgdc:Bounds x="380" y="185" width="100" height="60" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_endEvent" bpmnElement="endEvent">
        <omgdc:Bounds x="530" y="200" width="28" height="28" />
      </bpmndi:BPMNShape>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>
